{
  "name": "@mayarid/n8n-nodes-mayar",
  "version": "0.3.0",
  "description": "n8n community nodes for Mayar Headless API (actions & triggers)",
  "license": "MIT",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "icons/*"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json && cp nodes/mayar.svg dist/nodes/mayar.svg",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "dev": "tsc -p tsconfig.json -w"
  },
  "keywords": [
    "n8n",
    "n8n-nodes",
    "mayar",
    "headless",
    "api",
    "webhook",
    "trigger"
  ],
  "engines": {
    "node": ">=18"
  },
  "exports": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mayarid/n8n-node-mayar.git"
  },
  "bugs": {
    "url": "https://github.com/mayarid/n8n-node-mayar/issues"
  },
  "homepage": "https://github.com/mayarid/n8n-node-mayar#readme",
  "dependencies": {
    "n8n-workflow": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "n8n": {
    "nodes": [
      "dist/nodes/Mayar.node.js",
      "dist/nodes/MayarTrigger.node.js"
    ],
    "credentials": [
      "dist/credentials/MayarApi.credentials.js"
    ]
  }
}
